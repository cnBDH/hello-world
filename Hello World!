//C
#include<stdio.h>
int main(){
  print("Hello World!");
  return 0;
 }
 //C++
 #include<iostream>
 using namespace std;
 int main(){
  cout<<"Hello World!"<<endl;
  return 0;
 }
 //Java
 public class Main(){
    public static void main(String[args]){
      system.out.println("Hello World!");
    }
 }
 //Python
 print("Hello World!")
 
  
